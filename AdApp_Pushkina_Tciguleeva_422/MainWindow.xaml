<Window x:Class="AdApp_Pushkina_Tciguleeva_422.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AdApp_Pushkina_Tciguleeva_422"
        mc:Ignorable="d"
        Title="Объявления" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <StackPanel Orientation="Horizontal" DockPanel.Dock="Top" Margin="10">

            <StackPanel DockPanel.Dock="Top" HorizontalAlignment="Left" Margin="10">
                <Image Source="Лого.png"
                    Width="89" Height="50"
                    Margin="0,0,10,0"/>
            </StackPanel>
                <StackPanel>
                    <Button Content="Вход" Click="LoginButton_Click" Width="130"/>
                </StackPanel>
                <StackPanel>
                        <Button x:Name="CompletedButton"
                                    Content="Завершённые"
                                    Margin="0 0 16 0"
                                    Visibility="Collapsed"
                                    Click="CompletedButton_Click"/>
                </StackPanel>

            </StackPanel>


            <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden"/>
        </DockPanel>
    </Grid>
</Window>
